port <%= @redis_port %>
#bind <%= node[:redis][:bind] %>
logfile <%= node[:redis][:log_dir] %>/<%= @redis_port %>.log
loglevel <%= node[:redis][:loglevel] %>
dir <%= node[:redis][:data_dir] %>/<%= @redis_port %>

daemonize no
logfile stdout
databases 1
save 900 1
save 300 10
save 60 10000
rdbcompression yes
dbfilename dump.rdb
